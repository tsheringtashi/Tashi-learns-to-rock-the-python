Class Employee:
    def __init__(self, first, last, pay):
        self.first = first
        self.last = last
        self.pay = pay
        self.email = first +'.'+last+'@thimphu.com'
        
    def full_name(self):
        return '{} {}'.format(self.first, self.last)
        
emp_1 = Employee('yadap', 'Subiri', 60000)
emp_2 = Employee('Kinley', 'Tshering', 65000)

print(emp_1.fullname())
print(emp_2.fullname())
